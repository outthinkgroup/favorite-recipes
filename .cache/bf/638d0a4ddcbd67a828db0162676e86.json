s/add-recipe-button.scss","name":null,"original":{"line":98,"column":6},"generated":{"line":1,"column":3486}},{"source":"styles/add-recipe-button.scss","name":null,"original":{"line":99,"column":8},"generated":{"line":1,"column":3547}}],"sources":{"styles/global.scss":"@import \"shared.scss\";\n.recipe-title {\n  display: flex;\n  font-size: 20px;\n  font-weight: 600;\n  align-items: center;\n  a {\n    color: black;\n    &:hover {\n      color: #0fa3b1;\n    }\n  }\n  .count {\n    font-size: 16px;\n    display: inline-flex;\n    width: 2em;\n    min-width: 2em;\n    height: 2em;\n    line-height: 0;\n    align-items: center;\n    justify-content: center;\n    background: #2cadba;\n    color: white;\n    border-radius: 50%;\n    font-weight: bold;\n    margin-right: 20px;\n  }\n}\nbutton * {\n  pointer-events: none;\n}\n.icon {\n  svg {\n    width: 0.78em;\n    padding: 0;\n    fill: currentColor;\n  }\n}\n.icon-button {\n  .icon {\n    font-size: 18px;\n    pointer-events: none;\n  }\n  &.with-text {\n    display: flex;\n    align-items: center;\n    justify-content: var(--button-alignment, center);\n    > .icon {\n      display: inline-block;\n      padding: 0 var(--spacing-right, 5px) 0 var(--spacing-left, 5px);\n    }\n    &.icon-left {\n      --spacing-right: var(--icon-spacing, 5px);\n      --spacing-left: 0;\n    }\n    &.icon-right {\n      --spacing-left: var(--icon-spacing, 5px);\n      --spacing-right: 0;\n    }\n    .text {\n      width: var(--text-width, auto);\n    }\n  }\n}\n.add-recipe-to-list > .icon-button > .icon {\n  line-height: 0;\n  font-size: 30px;\n}\n\nform.generated-inline {\n  display: inline-flex;\n  width: 100%;\n  input {\n    width: 100%;\n  }\n  button {\n    * {\n      pointer-events: none;\n    }\n    opacity: 1;\n    pointer-events: all;\n    background: var(--button-color, #efefef);\n    width: auto;\n    border-radius: 0px;\n    color: var(--button-text-color, #888);\n    &:hover {\n      background: var(--button-color--hover, #e0e0e0);\n    }\n  }\n}\n\n.lists-action {\n  background: white;\n  display: flex;\n  align-items: center;\n  border-bottom: 1px solid #f0f0f0;\n  > button {\n    @include appearance(none);\n    width: 100%;\n    height: 100%;\n    background: transparent;\n    color: #888;\n    font-weight: 400;\n    font-size: 14px;\n    border-radius: 0px;\n    border: none;\n    &:hover {\n      background: #efefef;\n    }\n  }\n}\n@import \"add-recipe-button.scss\";\n","styles/shared.scss":"@mixin appearance($value) {\n  -webkit-appearance: $value;\n  -moz-appearance: $value;\n  appearance: $value;\n}\n","styles/states.scss":"///list Item states for idle, error, and loading\n%states {\n  &[data-state=\"loading\"] {\n    opacity: 0.75;\n  }\n  &[data-state=\"error\"] {\n    background: var(--danger-light, var(--danger, #df9393));\n  }\n  &[data-state=\"hidden\"] {\n    display: none;\n  }\n}\n","styles/add-recipe-button.scss":"@import \"states.scss\";\n\n.add-recipe-to-list {\n  position: relative;\n  max-width: 220px;\n  > button {\n    width: 100%;\n    border-radius: 3px;\n    padding: 0;\n    display: flex;\n    justify-content: space-between;\n    align-items: center;\n    > * {\n      font-weight: bold;\n      padding: 12px 15px;\n    }\n    &.no-text {\n      width: auto;\n    }\n    .button-body {\n      width: 100%;\n      text-align: center;\n      padding-left: 0;\n    }\n  }\n  .button-lists {\n    position: absolute;\n    top: calc(100% + 2px);\n    left: 0;\n    width: 100%;\n    background: white;\n    box-shadow: 0 1px 4px rgba(0, 0, 0, 0.4);\n    display: none;\n    z-index: 99;\n    border-radius: 3px;\n  }\n  .lists {\n    margin: 0;\n    padding: 0;\n    list-style: none;\n    font-size: 14px;\n    overflow: hidden;\n    overflow-y: scroll;\n    max-height: 220px;\n    li {\n      @extend %states;\n    }\n    button {\n      padding: 5px;\n      background: transparent;\n      width: 100%;\n      border-radius: 0;\n      border-bottom: 1px solid #f0f0f0;\n      text-transform: none;\n      text-align: left;\n      svg {\n        fill: #888;\n      }\n      &:hover {\n        background: rgba(0, 0, 0, 0.1);\n      }\n      * {\n        font-size: 14px;\n        color: black;\n        .count {\n          color: white;\n        }\n      }\n    }\n  }\n  &.list-on-side {\n    text-align: left;\n    width: auto;\n    display: inline-block;\n    .button-lists {\n      top: 0;\n      left: 100%;\n      width: 250px;\n    }\n  }\n\n  &[data-state=\"on\"] {\n    .button-lists {\n      display: unset;\n    }\n  }\n  .lists-action {\n    > button {\n      font-size: 12px;\n      padding: 8px;\n      --button-alignment: flex-start;\n      --icon-spacing: 0px;\n      --text-width: 100%;\n    }\n    .small-inline-input {\n      padding: 5px;\n      font-size: 16px;\n      + button {\n        padding: 5px 10px;\n      }\n    }\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"0dea6b3d25a9108b189ddab7c0350658","cacheData":{"env":{}}}